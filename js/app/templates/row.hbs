<tr>
  <td class="time">{{ this.hours }}:{{ this.minutes }}</td>
  {{#each this.days }}
    <td>
      {{#each this }}
        <div class="e{{this.slot}}of{{this.max}}" style="height: {{ this.height }}px;">
          <span class="name">{{ this.name }}</span>
          <span class="schedule">{{ this.schedule }}</span>
        </div>
      {{/each }}
    </td>
  {{/each }}
</tr>
